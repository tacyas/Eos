typedef struct lctfDetermineInfo {
	int    degree;
	double step;
	double max;
	double min;
	float  firstRingDefocus;
	float  IntensityMin;
	float  IntensityMax;
	float* noise;
} lctfDetermineInfo;

/*prototype begin*/
extern void ctfInfoWrite2(FILE* fpt,lctfDetermineInfo linfo, char* head);
/*prototype end*/

