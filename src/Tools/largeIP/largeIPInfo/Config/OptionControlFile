# OptionControlFile
# FileFormat
"-i","-i[nput]","InputDataFile","Essential","1","1","In","inFile","NULL"
"-o","-o[utput]","OutputDataFile","Optional","1","1","Out","outFile","stdout"
"-start","-start","StartCoord","Optional","2","1","startx","Integer","0","2","starty","Integer","0"
"-step","-step","StepCoord","Optional","2","1","stepx","Integer","1","2","stepy","Integer","1"
"-size","-size","SizeCoord","Optional","2","1","width","Integer","LARGE_IP_IMAGE_WIDTH","2","height","Integer","LARGE_IP_IMAGE_HEIGHT"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-m","-m[ode]","Mode","Optional","1","1","mode","Integer","0"
