	@../$(OSTYPE)/$(OBJECTNAME)	 -h

exec:
	@echo "----- Execution Check -----"
	../$(OSTYPE)/$(OBJECTNAME) -i data/test.dm3
	../$(OSTYPE)/$(OBJECTNAME) -i data/test.dm4
	@echo "----- Calc check -----"		

clean:
