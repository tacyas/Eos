# FileFormat
"-i","-i[nput]","Input: Imod Store ","Essential","1","1","In","inFile","NULL"
"-o","-o[utput]","Output: MRC ","Optional","1","1","Out","outFile","NULL"
"-O","-O[utput]","Output: MRC LIST","Optional","0"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-m","-m[ode]","Mode","Optional","1","1","mode","Integer","0"
