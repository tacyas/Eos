# OptionControlFile
# FileFormat
"-i","-i[nput]","InputDataFile","Essential","1","1","inFile","inFile","NULL"
"-o","-o[utput]","OutputDataFile","Essential","1","1","outFile","outFile","NULL"
"-r","-r[esolution]","Resolution[A]","Optional","1","1","Resolution","Real","1.0"
"-r3","-r[esolution]3","Resolution[A]","Optional","3","1","ResolutionX","Real","1.0","1","ResolutionY","Real","1.0","1","ResolutionZ","Real","1.0"
"-t","-t[ime]","x OptimalTime","Optional","1","1","time","Real","1.0"
"-d","-d[irnum]","tiffDirectoryNumber","Optional","1","1","dirnum","Integer","-1"
"-I","-I[nverse]","Black<->White","Optional","0"
"-Signed","-Signed","Original image is Signed or Not: Default unsigned","Optional","0"
"-Offset","-Offset","OffsetValue","Optional","1","1","Offset","Real","0.0"
"-Max","-Max","MaxValue:2^Max","Optional","1","1","Max","Real","16"
"-Min","-Min","MinValue:2^Min: without 0","Optional","1","1","Min","Real","1"
"-realMin","-realMin","Real MinValue","Optional","1","1","realMin","Real","0.0"
"-realMax","-realMax","Real MaxValue","Optional","1","1","realMax","Real","0.0"
"-realMaxBit","-realMaxBit","Real MaxValue Bit","Optional","1","1","realMaxBit","Real","14"
"-sin","-sin","sin file from Zeiss","Optional","1","1","sinFile","inFile","NULL"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-S","-S[hrink]","ShrinkMode","Optional","1","1","Shrink","Integer","8"
"-M","-M[ode]","Mode: mrcMode: Def: if -m 0, -M 0. if -m 1 -M 2","Optional","1","1","Mode","Integer","0"
"-m","-m[ode]","Mode: 0 (Direct), 1(Log10(65535/data))","Optional","1","1","mode","Integer","0"
