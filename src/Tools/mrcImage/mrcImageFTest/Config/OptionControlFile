# OptionControlFile
# FileFormat
"-i","-i[nput]","IN: InputDataFile","Essential","1","1","In","inFileList","NULL"
"-r","-r[ef]","Ref: InputDataFile","Essential","1","1","Ref","inFileList","NULL"
"-o","-o[utput]","Output: Probability","Essential","1","1","Out","outFile","NULL"
"-f","-f[Value]","Output: F-Value","Optional","1","1","OutF","outFile","NULL"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-m","-m[ode]","Mode","Optional","1","1","mode","Integer","0"
