# OptionControlFile
# FileFormat
"-i","-i[nput]","Input: mrc","Essential","1","1","In","inFile","NULL"
"-o","-o[utput]","Output: mrc","Essential","1","1","Out","outFile","NULL"
"-EA","-E[uler]A[ngle]","Input: Euler Angle","Optional","4","1","EulerAngle","String","YOYS","2","Rot1","Real","0.0","3","Rot2","Real","0.0","4","Rot3","Real","0.0"
"-N","-N","OutputImageSize","Optional","3","1","Nx","Integer","-1","2","Ny","Integer","-1","3","Nz","Integer","-1" 
"-M","-M[ode]","InterpolationMode","Optional","1","1","Mode","Integer","2"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-m","-m[ode]","Mode","Optional","1","1","mode","Integer","0"
