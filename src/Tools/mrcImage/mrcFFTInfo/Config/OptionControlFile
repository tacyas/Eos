# OptionControlFile
# FileFormat
"-i","-i[nput]","Input","Essential","1","1","In","inFile::mrcImage","NULL"
"-o","-o[utput]","Output","Essential","1","1","Out","outFile::ASCII","NULL"
"-a","-a[ngle]","Angle","Optional","1","1","Angle","Real","0.0"
"-nDiv","-nDiv[ision]","num of Divsion","Optional","1","1","nDivision","Integer","18"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-m","-m[ode]","Mode","Optional","1","1","mode","Integer","0"
