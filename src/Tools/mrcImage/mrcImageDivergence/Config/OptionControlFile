"-i2","-i[nput]2","Input::dx,dy","Optional","2","1","In2x","inFile::mrcImage","NULL","2","In2y","inFile::mrcImage","NULL"
"-i3","-i[nput]3","Input::dx,dy","Optional","3","1","In3x","inFile::mrcImage","NULL","2","In3y","inFile::mrcImage","NULL","3","In3z","inFile::mrcImage","NULL"
"-o","-o[utput]","OutputDataFile","Essential","1","1","Out","outFile","NULL"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-m","-m[ode]","Mode","Optional","1","1","mode","Integer","0"
