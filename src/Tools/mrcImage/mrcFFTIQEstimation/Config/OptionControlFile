# OptionControlFile
# FileFormat
"-i","-i[nput]","Input: MRC","Essential","1","1","In","inFile","NULL"
"-sp","-sp[otList]","Input: SpotList: X Y Z","Essential","1","1","SpotList","inFile","stdin"
"-radius","-radius","Input: [pixel]","Optional","1","1","radius","Integer","3"
"-o","-o[utput]","Output: ASCII","Essential","1","1","Out","outFile","NULL"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-m","-m[ode]","Mode","Optional","1","1","mode","Integer","0"
