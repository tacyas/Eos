# OptionControlFile
# FileFormat
"-i","-i[nput]","Input: mrcImage","Essential","1","1","In","inFile","NULL"
"-ika","-i[nput]k[ernal]a[rray]","Input: Array","Optional","1","1","InKernelArray","inFile","NULL"
"-ikm","-i[nput]k[ernal]m[rc]","Input: mrcImage","Optional","1","1","InKernelMRC","inFile","NULL"
"-o","-o[utput]","OutputDataFile","Essential","1","1","Out","outFile","NULL"
"-times","-times","Repeat time","Optional","1","1","times","Integer","1"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-m","-m[ode]","Mode","Optional","1","1","mode","Integer","0"
