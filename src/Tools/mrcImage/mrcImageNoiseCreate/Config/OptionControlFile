# OptionControlFile
# FileFormat
"-r","-r[eference]","ReferenceDataFile","Essential","1","1","In","inFile","NULL"
"-o","-o[utput]","OutputDataFile","Essential","1","1","Out","outFile","NULL"
"-s","-s[tandard deviation]","Standard deviation of noise","Optional","1","1","Noise","Real","1.0"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-m","-m[ode]","Mode","Optional","1","1","mode","Integer","0"
