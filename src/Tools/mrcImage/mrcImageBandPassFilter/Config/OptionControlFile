# OptionControlFile
# FileFormat
"-i","-i[nput]","InputDataFile","Essential","1","1","In","inFile","NULL"
"-o","-o[utput]","OutputDataFile","Essential","1","1","Out","outFile","NULL"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-m","-m[ode]","Mode","Optional","1","1","mode","Integer","0"
"-hvl","-h[alf]v[aluepoint]l[ow]","HalfValuePointLow","Optional","1","1","hvl","Real","1.0"
"-hvh","-h[alf]v[aluepoint]h[igh]","HalfValuePointHigh","Optional","1","1","hvh","Real","1.0"
"-wl","-w[idth]l[ow]","WidthOfDumpingLow","Optional","1","1","wl","Real","1.0"
"-wh","-w[idth]h[igh]","WidthOfDumpingHigh","Optional","1","1","wh","Real","1.0"
"-lowweight","-lowweight","Weight of Low Freq.","Optional","1","1","lowWeight","Real","0.0"
