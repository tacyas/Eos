# OptionControlFile
# FileFormat
"-i","-i[nput]","llData:InputDataFile","Essential","1","1","In","inFile","NULL"
"-r","-r[eference]","llData:Reference","Essential","1","1","Ref","inFile","NULL"
"-o","-o[utput]","llData(Normalized In):OutputDataFile","Optional","1","1","Out","outFile","NULL"
"-dif","-dif[ffence]","llData(Difference):OutputDataFile","Optional","1","1","Dif","outFile","NULL"
"-O","-O[utput]","FittedParameter","Optional","1","1","OutParam","outFile","stdout"
"-c","-c[onfig]","ConfigurationFile","Optional","1","1","configFile","inFile","NULL"
"-m","-m[ode]","Mode","Optional","1","1","mode","Integer","0"
